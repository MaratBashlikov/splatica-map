{
  "version": 8,
  "name": "Splatica Dark Neon",
  "metadata": {
    "mapbox:autocomposite": true,
    "mapbox:type": "template",
    "maptiler:copyright": "© MapTiler © OpenStreetMap contributors"
  },
  "sources": {
    "maptiler": {
      "type": "raster",
      "tiles": [
        "https://api.maptiler.com/maps/darkmatter/{z}/{x}/{y}.png?key=MAPTILER_KEY_PLACEHOLDER"
      ],
      "tileSize": 256,
      "attribution": "© MapTiler © OpenStreetMap contributors"
    },
    "openmaptiles": {
      "type": "vector",
      "url": "https://api.maptiler.com/tiles/v3-openmaptiles/tiles.json?key=MAPTILER_KEY_PLACEHOLDER"
    }
  },
  "sprite": "https://api.maptiler.com/maps/darkmatter/sprite",
  "glyphs": "https://api.maptiler.com/fonts/{fontstack}/{range}.pbf?key=MAPTILER_KEY_PLACEHOLDER",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#020617"
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "paint": {
        "fill-color": "#020617",
        "fill-opacity": 0.8
      }
    },
    {
      "id": "landcover",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "paint": {
        "fill-color": "#0f172a",
        "fill-opacity": 0.3
      }
    },
    {
      "id": "landuse",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "paint": {
        "fill-color": "#0f172a",
        "fill-opacity": 0.2
      }
    },
    {
      "id": "park",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "park",
      "paint": {
        "fill-color": "#0f172a",
        "fill-opacity": 0.15
      }
    },
    {
      "id": "boundary-country",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "filter": ["==", "admin_level", 2],
      "paint": {
        "line-color": "#1e40af",
        "line-width": 1,
        "line-opacity": 0.3,
        "line-blur": 0.5
      }
    },
    {
      "id": "boundary-state",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "filter": ["==", "admin_level", 4],
      "paint": {
        "line-color": "#1e40af",
        "line-width": 0.5,
        "line-opacity": 0.15
      }
    },
    {
      "id": "road-path",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "path", "track"],
      "paint": {
        "line-color": "#1f2933",
        "line-width": {
          "base": 1.2,
          "stops": [[12, 0.5], [20, 2]]
        },
        "line-opacity": 0.2
      }
    },
    {
      "id": "road-minor",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "minor", "service"],
      "paint": {
        "line-color": "#1f2933",
        "line-width": {
          "base": 1.2,
          "stops": [[12, 1], [20, 4]]
        },
        "line-opacity": 0.25
      }
    },
    {
      "id": "road-secondary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "secondary", "tertiary"],
      "paint": {
        "line-color": "#475569",
        "line-width": {
          "base": 1.2,
          "stops": [[10, 1.5], [20, 6]]
        },
        "line-opacity": 0.4
      }
    },
    {
      "id": "road-primary-glow",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "primary", "trunk"],
      "paint": {
        "line-color": "#38bdf8",
        "line-width": {
          "base": 1.2,
          "stops": [[10, 3], [20, 12]]
        },
        "line-opacity": 0.3,
        "line-blur": 2
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "primary", "trunk"],
      "paint": {
        "line-color": "#0ea5e9",
        "line-width": {
          "base": 1.2,
          "stops": [[10, 2], [20, 8]]
        },
        "line-opacity": 0.8
      }
    },
    {
      "id": "road-motorway-glow",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "motorway"],
      "paint": {
        "line-color": "#22d3ee",
        "line-width": {
          "base": 1.2,
          "stops": [[10, 4], [20, 16]]
        },
        "line-opacity": 0.4,
        "line-blur": 3
      }
    },
    {
      "id": "road-motorway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "motorway"],
      "paint": {
        "line-color": "#06b6d4",
        "line-width": {
          "base": 1.2,
          "stops": [[10, 3], [20, 12]]
        },
        "line-opacity": 0.9
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "building",
      "minzoom": 14,
      "paint": {
        "fill-color": "#1e1b4b",
        "fill-opacity": 0.2
      }
    },
    {
      "id": "3d-buildings",
      "type": "fill-extrusion",
      "source": "openmaptiles",
      "source-layer": "building",
      "minzoom": 14,
      "paint": {
        "fill-extrusion-color": "#312e81",
        "fill-extrusion-opacity": 0.3,
        "fill-extrusion-height": [
          "interpolate",
          ["linear"],
          ["zoom"],
          14,
          3,
          16,
          ["*", ["get", "render_height"], 0.5],
          20,
          ["*", ["get", "render_height"], 0.8]
        ],
        "fill-extrusion-base": [
          "case",
          [">", ["get", "render_min_height"], 0],
          ["get", "render_min_height"],
          0
        ]
      }
    },
    {
      "id": "place-city-lg",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "filter": ["==", "class", "city"],
      "minzoom": 4,
      "layout": {
        "text-field": "{name}",
        "text-font": ["Noto Sans Regular"],
        "text-size": {
          "stops": [[4, 12], [10, 16]]
        },
        "text-transform": "uppercase",
        "text-letter-spacing": 0.1
      },
      "paint": {
        "text-color": "#e5e7eb",
        "text-halo-color": "#020617",
        "text-halo-width": 2,
        "text-opacity": 0.9
      }
    },
    {
      "id": "place-city-md",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "filter": ["==", "class", "town"],
      "minzoom": 8,
      "layout": {
        "text-field": "{name}",
        "text-font": ["Noto Sans Regular"],
        "text-size": {
          "stops": [[8, 11], [12, 14]]
        }
      },
      "paint": {
        "text-color": "#94a3b8",
        "text-halo-color": "#020617",
        "text-halo-width": 1.5,
        "text-opacity": 0.7
      }
    }
  ]
}

